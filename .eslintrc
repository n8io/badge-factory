{
  "env": {
    "browser": true,
    "es6": true,
    "jasmine": true,
    "mocha": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "array-bracket-spacing": [
      2,
      "never"
    ],
    "arrow-body-style": [
      2,
      "always"
    ],
    "arrow-parens": [
      2,
      "always"
    ],
    "arrow-spacing": [
      2,
      {
        "before": true,
        "after": true
      }
    ],
    "block-spacing": [
      2,
      "always"
    ],
    "brace-style": [
      2,
      "stroustrup",
      {
        "allowSingleLine": false
      }
    ],
    "camelcase": 2,
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": [
      2,
      "last"
    ],
    "curly": [
      2,
      "all"
    ],
    "default-case": 2,
    "dot-location": [
      2,
      "property"
    ],
    "dot-notation": [
      2,
      {
        "allowPattern": "^_?[a-z]+(_[a-z]+)+$"
      }
    ],
    "eol-last": 2,
    "eqeqeq": 2,
    "guard-for-in": 2,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "max-len": [
      2,
      180,
      4,
      {
        "ignoreComments": true,
        "ignoreUrls": true,
        "ignorePattern": "^\\s*var\\s.+=\\s*require\\s*\\("
      }
    ],
    "newline-after-var": 2,
    "no-array-constructor": 2,
    "no-arrow-condition": 2,
    "no-const-assign": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-invalid-this": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-multiple-empty-lines": [
      2,
      {
        "max": 2
      }
    ],
    "no-new-object": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": 2,
    "no-unused-vars": 1,
    "no-use-before-define": [
      2,
      "nofunc"
    ],
    "no-var": 2,
    "no-with": 2,
    "object-curly-spacing": [
      2,
      "never"
    ],
    "object-shorthand": [
      2,
      "never"
    ],
    "operator-linebreak": [
      2,
      "before",
      {
        "overrides": {
          "?": "after"
        }
      }
    ],
    "padded-blocks": [
      2,
      "never"
    ],
    "prefer-const": 2,
    "prefer-template": 1,
    "quotes": [
      2,
      "single"
    ],
    "semi": 2,
    "semi-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "space-after-keywords": [
      2,
      "always"
    ],
    "space-before-blocks": [
      2,
      "always"
    ],
    "space-before-function-paren": [
      2,
      "never"
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "space-infix-ops": 2,
    "space-return-throw-case": 2,
    "space-unary-ops": [
      1,
      {
        "words": true,
        "nonwords": false
      }
    ],
    "spaced-comment": [
      2,
      "always"
    ]
  }
}
